	<modelVersion>4.0.0</modelVersion>
	<groupId>PriceBook</groupId>
	<artifactId>PriceBook</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>war</packaging>
	<name>PriceBook</name>
	<description>PriceBook maven project.</description>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<build>
		<sourceDirectory>src</sourceDirectory>
		<testSourceDirectory>test</testSourceDirectory>
		<resources>
			<resource>
				<directory>src</directory>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<!-- <testResources> <testResource> <directory>test</directory> <excludes> 
			<exclude>**/*.java</exclude> </excludes> </testResource> </testResources> -->
		<plugins>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<warSourceDirectory>WebContent</warSourceDirectory>
					<failOnMissingWebXml>false</failOnMissingWebXml>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
	<profile>
		<id>test</id>
		<activation>
	            <activeByDefault>true</activeByDefault>
	      </activation>
		<build>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-antrun-plugin</artifactId>
					<version>1.8</version>
					<executions>
						<execution>
							<phase>test</phase>
							<goals>
								<goal>run</goal>
							</goals>
							<configuration>
								<target>
									<echo>Copy pricebook_test.properties</echo>
									<copy overwrite="true" file="src/pricebook_test.properties" tofile="${project.build.outputDirectory}/pricebook.properties"/>
									<echo>Copy jdbc_test.properties</echo>
									<copy overwrite="true" file="src/jdbc_test.properties" tofile="${project.build.outputDirectory}/jdbc.properties"/>
									<echo>Copy log4j_test.properties</echo>
									<copy overwrite="true" file="src/log4j_test.properties" tofile="${project.build.outputDirectory}/log4j.properties"/>
									<echo>Copy avatax_test.properties</echo>
									<copy overwrite="true" file="src/avatax_test.properties" tofile="${project.build.outputDirectory}/avatax_test.properties"/>
									<echo>Copy ApplicationResources_test.properties</echo>
									<copy overwrite="true" file="src/com/wishtree/pricebook/resources/ApplicationResources_test.properties" tofile="${project.build.outputDirectory}/com/wishtree/pricebook/resources/ApplicationResources.properties"/>
									<echo>Copy email_test.properties</echo>
									<copy overwrite="true" file="src/com/wishtree/pricebook/resources/email_test.properties" tofile="${project.build.outputDirectory}/com/wishtree/pricebook/resources/email.properties"/>
									</target>
							</configuration>
						</execution>
					</executions>
				</plugin>
			</plugins>
		</build>
	</profile>
	<profile>
		<id>dev</id>
		<activation>
	            <activeByDefault>true</activeByDefault>
	      </activation>
		<build>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-antrun-plugin</artifactId>
					<version>1.8</version>
					<executions>
						<execution>
							<phase>test</phase>
							<goals>
								<goal>run</goal>
							</goals>
							<configuration>
								<target>
									<echo>Copy pricebook_dev.properties</echo>
									<copy overwrite="true" file="src/pricebook_dev.properties" tofile="${project.build.sourceDirectory}/pricebook.properties"/>
									<echo>Copy jdbc_dev.properties</echo>
