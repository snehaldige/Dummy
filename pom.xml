									<echo>Copy avatax_dev.properties</echo>
									<copy overwrite="true" file="src/avatax_dev.properties" tofile="${project.build.outputDirectory}/avatax_dev.properties"/>
									<echo>Copy ApplicationResources_dev.properties</echo>
									<copy overwrite="true" file="src/com/wishtree/pricebook/resources/ApplicationResources_dev.properties" tofile="${project.build.sourceDirectory}/com/wishtree/pricebook/resources/ApplicationResources.properties"/>
									<echo>Copy email_dev.properties</echo>
									<copy overwrite="true" file="src/com/wishtree/pricebook/resources/email_dev.properties" tofile="${project.build.sourceDirectory}/com/wishtree/pricebook/resources/email.properties"/>
								</target>
							</configuration>
						</execution>
					</executions>
				</plugin>
			</plugins>
		</build>
	</profile>
	<profile>
		<id>staging</id>
		<activation>
	            <activeByDefault>true</activeByDefault>
	      </activation>
		<build>
			<plugins>
